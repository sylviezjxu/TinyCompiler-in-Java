main
var a, b, c;
{
    let a <- call InputNum();
    let b <- call InputNum();
    let c <- b;

    if b < a then
        let b <- b + 1;
        while a < 100 do
            let b <- a + 1;
            let b <- c + 1;
        od;
    else
       while b < 100 do
           let b <- a + 1;
           let b <- c + 1;
       od;
       let b <- b - 1;
    fi;

    call OutputNum(a);
    call OutputNum(b);
    call OutputNum(c);
}.
